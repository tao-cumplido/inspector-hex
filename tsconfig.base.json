{
	"compilerOptions": {
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"useUnknownInCatchVariables": true,
		"noImplicitOverride": true,
		"module": "nodenext",
		"isolatedModules": true,
		"exactOptionalPropertyTypes": true,
		"lib": ["esnext"],
		"skipLibCheck": true
	},
	"exclude": ["node_modules", ".vscode-test"]
}
