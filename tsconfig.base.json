{
	"compilerOptions": {
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"useUnknownInCatchVariables": true,
		"noImplicitOverride": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"module": "commonjs",
		"target": "es2020",
		"lib": ["es2020"],
		"sourceMap": true,
		"composite": true
	},
	"exclude": ["node_modules", ".vscode-test"]
}
